<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>AgeServlet</servlet-name>
        <servlet-class>org.voiculescu.siit.wk17.Age.AgeServlet</servlet-class>
        <init-param>
            <param-name>birthDate</param-name>
            <param-value>1983-09-23</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>AgeServlet</servlet-name>
        <url-pattern>/myAge</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>myAge</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

</web-app>
